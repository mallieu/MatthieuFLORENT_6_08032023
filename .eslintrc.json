{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "unused-imports"
      ],
    "rules": {
        "unused-imports/no-unused-imports": "warn",
        "unused-imports/no-unused-vars": "error"
      }
}
